<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="dermatologyapp.shahanchor.com.dermatologist">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".DermatologyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/MyAppTheme">
        <activity android:name=".login.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Register.RegisterActivity" />
        <activity android:name=".feedback.FeedbackActivity" />
        <activity android:name=".home.HomeActivity" />
        <activity android:name=".admin_home.AdminHomeActivity" />
        <activity android:name=".showUsers.ShowUsers" />
        <activity android:name=".showUsers.UserDetailDescription"></activity>
    </application>


</manifest>